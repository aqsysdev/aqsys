<!--

打刻テンプレート

-->
<script type="text/javascript">
  var tnum={{tnum}};
  var seqnum={{seqnum}};
</script>
{{#if tnum==1}}
<h2 class="page-header">ナンバー入力チーム１</h2>
{{#elif tnum==2}}
<h2 class="page-header">打刻チーム１</h2>
{{#elif tnum==3}}
<h2 class="page-header">ナンバー入力チーム２</h2>
{{#elif tnum==4}}
<h2 class="page-header">打刻チーム２</h2>
{{else}}
<h2 class="page-header">{{tnum}}</h2>
{{/if}}
<div id="msg_list" style="height:280px; overflow:scroll;">
  <table id="recordlist">
  {{#each recordlist}}
    {{#if seqnum}}
    <tr class='record'>
      <td class='seqnum'>{{seqnum}}</td>
      <td class='racenum'>{{racenum}}</td>
      <td class='ftime'>{{ftime}}</td>
    </tr>
    {{else}}
    <tr>
      <th>順番</th>
      <th>レース番号</th>
      <th>打刻時刻</th>
    </tr>
    <tr id="startTime">
      <th></th>
      <th>start time</th>
      <th>{{ftime}}</th>
    </tr>
    {{/if}}
  {{/each }}
</table>
</div>
 <form action="" method="put" onsubmit="return false;">
   <input type="text" class="text" style="width:72px; padding:10px"
   id="message" value=""/>
   <input type="submit" class="button btn-primary btn-lg" style="padding:10px" onclick="punchTime({{tnum}});" value="送信" />
 </form>
</div>
<script type="text/javascript" src="/js/punch.js"></script>
